---
name: docker-broker
director_uuid: "{{ cf_director_uuid }}"

releases:
 - name: docker
   version: latest

compilation:
  workers: 1
  network: default
  reuse_compilation_vms: true
  cloud_properties:
    instance_type: "{{ docker_instance_type }}"

update:
  canaries: 0
  canary_watch_time: 30000-60000
  update_watch_time: 30000-60000
  max_in_flight: 1
  serial: false

networks:
{% if docker_fp is defined %}
  - name: floating
    type: vip
{% endif %}
  - name: default
    type: manual
    subnets:
      - range: "{{ docker_cidr }}"
        gateway: "{{ docker_cidr | nthhost(1) }}"
        dns: [{{ cf_dns | join(', ') }}]
        reserved: ["{{ docker_cidr | nthhost(2) }} - {{ docker_cidr | nthhost(3) }}"]
        static: ["{{ docker_ip }}"]
        cloud_properties:
{% if (provider == 'aws') %}
          subnet: "{{ docker_subnet_id }}"
{% elif (provider == 'openstack') %}
          net_id: "{{ docker_subnet_id }}"
{% endif %}
          security_groups:
            - "{{ cf_bosh_security_group }}"
            - "{{ docker_broker_security_group }}"

resource_pools:
  - name: default
    network: default
    stemcell:
      name: "{{ cf_bosh_stemcell_name }}"
      version: "{{ cf_bosh_stemcell_version }}"
    cloud_properties:
      instance_type: "{{ docker_instance_type }}"

  - name: errand
    network: default
    stemcell:
      name: "{{ cf_bosh_stemcell_name }}"
      version: "{{ cf_bosh_stemcell_version }}"
    cloud_properties:
      instance_type: "{{ docker_errand_instance_type }}"

jobs:
  - name: broker
    templates:
      - name: docker
      - name: cf-containers-broker
    instances: 1
    resource_pool: default
    persistent_disk: 65536
    networks:
      - name: default
        default: [dns, gateway]
        static_ips:
          - "{{ docker_ip }}"
{% if docker_fp is defined %}
      - name: floating
        static_ips:
          - "{{ docker_fp }}"
{% endif %}

  - name: fetch-containers-images
    template: fetch-containers-images
    instances: 1
    resource_pool: errand
    lifecycle: errand
    networks:
      - name: default

  - name: broker-registrar
    template: broker-registrar
    instances: 1
    resource_pool: errand
    lifecycle: errand
    networks:
      - name: default
        default: [dns, gateway]

  - name: broker-deregistrar
    template: broker-deregistrar
    instances: 1
    resource_pool: errand
    lifecycle: errand
    networks:
      - name: default
        default: [dns, gateway]

properties:
  nats:
    user: nats
    password: "{{ cf_password }}"
    port: 4222
    machines:
      - "{{ nats_ip }}"

{% if (provider == 'openstack') %}
  env:
    http_proxy: "{{ http_proxy }}"
    https_proxy: "{{ https_proxy }}"
    no_proxy: "{{ no_proxy }}"
{% endif %}

  cf:
    api_url: "http://api.{{ cf_system_domain }}"
    admin_username: 'admin'
    admin_password: 'admin'

  docker:
    insecure_registries:
      - "docker-registry.{{ cf_system_domain }}"
    tcp_address: 0.0.0.0
    ulimit:
      nofile: 65536

  fetch_containers_images:
    docker_url: tcp://{{ docker_ip }}:4243

  broker:
    name: cf-containers-broker
    fetch_images: false
    protocol: http
    host: "cf-containers-broker.{{ cf_system_domain }}"
    port: 80
    username: 'containers'
    password: 'containers'
    cookie_secret: 'e7247dae-a252-4393-afa3-2219c1c02efd'
